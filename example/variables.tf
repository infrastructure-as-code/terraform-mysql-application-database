variable "mysql_host" {
  type        = string
  description = "MySQL hostname"
}

variable "mysql_port" {
  type        = number
  description = "MySQL port"
}

variable "mysql_user" {
  type        = string
  description = "MySQL admin username"
}

variable "mysql_password" {
  type        = string
  description = "MySQL admin user password"
}
